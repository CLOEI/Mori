[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../mori/target/debug/libmori.so"
linux.release.x86_64 =   "res://../mori/target/release/libmori.so"
windows.debug.x86_64 =   "res://../mori/target/debug/mori.dll"
windows.release.x86_64 = "res://../mori/target/release/mori.dll"
macos.debug =            "res://../mori/target/debug/libmori.dylib"
macos.release =          "res://../mori/target/release/libmori.dylib"
macos.debug.arm64 =      "res://../mori/target/debug/libmori.dylib"
macos.release.arm64 =    "res://../mori/target/release/libmori.dylib"
